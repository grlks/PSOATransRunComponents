Document (
  Prefix(isopl: <https://www.iso.org/standard/21413.html#>)
  Prefix(: <http://psoa.ruleml.org/test/external-isopl#>)
  
  Group (
    Forall ?X1 ?X2 ?X3 (
      :inRange(?X1 ?X2 ?X3) :-
        And(External(isopl:greater_than_or_eq(?X1 ?X2))
            External(isopl:less_than_or_eq(?X1 ?X3)))
    )
  )
)