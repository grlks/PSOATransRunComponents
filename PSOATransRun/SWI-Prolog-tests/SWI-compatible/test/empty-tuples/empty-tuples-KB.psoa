RuleML (
  Assert (
    o#p()
    o#p(+[])
    o#p(-[])
    o#p(a)
    o#p(+[a])
    o#p(-[a])
    o#p(a b)
    o#p(+[a b])
    o#p(-[a b])
    o#p(a b c)
    o#p(+[a b c])
    o#p(-[a b c])
    o#p(+[] +[a] +[a b] +[a b c] -[] -[a] -[a b] -[a b c])
    Forall ?o (
      ?o#q(+[]) :-
        And(?o#p(-[])
            o#p(+[] +[a] +[a b] +[a b c] -[] -[a] -[a b] -[a b c])
        )
    )
    r()
    r(+[])
    r(-[])
    r(a)
    r(+[a])
    r(-[a])
    r(a b)
    r(+[a b])
    r(-[a b])
    r(a b c)
    r(+[a b c])
    r(-[a b c])
    r(+[] +[a] +[a b] +[a b c] -[] -[a] -[a b] -[a b c])
    s(+[]) :-
        And(r(-[])
            r(+[] +[a] +[a b] +[a b c] -[] -[a] -[a b] -[a b c])
        )
  )
)